{
    "name": "4xxi/rest-request-errors",
    "description": "Bundle providing serialization of REST request errors",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.1.3",
        "symfony/serializer": "^4.0",
        "symfony/http-foundation": "^4.0",
        "symfony/http-kernel": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/config": "^4.0"
    },
    "suggest": {
        "symfony/form": "Enable support for forms"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Fourxxi\\RestRequestError\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fourxxi\\RestRequestError\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Vozhzhov",
            "email": "avozhzhov@4xxi.com"
        },
        {
            "name": "Dmitrii Bezmelnitsyn",
            "email": "dbezmelnitsyn@4xxi.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "symfony/form": "^4.0"
    }
}
